#include <stdio.h>
#include <math.h>
const int n = 6;

// bubblesort
void bubblesort(int arr[]);
void swap(int *a, int *b);
void display(int arr[]);

int main()
{
    int arr[] = {78, 45, 96, 32, 93, 44};
    display(arr);
    bubblesort(arr);
    display(arr);
    return 0;
}

void bubblesort(int arr[])
{
    for (int i = 0; i < n - 1; i++)
    {
        for (int j = 0; j < n - i - 1; j++)
        {
            if (arr[j] > arr[j + 1])
            {
                swap(&arr[j], &arr[j + 1]);
            }
        }
    }
}

void swap(int *a, int *b)
{
    int temp = *a;
    *a = *b;
    *b = temp;
}

void display(int arr[])
{
    for (int i = 0; i < n; i++)
    {
        printf("%d ", arr[i]);
    }
    printf("\n");
}
